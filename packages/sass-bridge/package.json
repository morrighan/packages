{
    "name": "@cichol/sass-bridge",
    "version": "4.5.0",
    "description": "A bridge interface to define Sass custom functions.",
    "homepage": "https://github.com/morrighan/packages",
    "bugs": {
        "url": "https://github.com/morrighan/packages/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "type": "module",
    "main": "./dists/index.cjs",
    "module": "./dists/index.js",
    "types": "./dists/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "default": "./dists/index.js",
                "types": "./dists/index.d.ts"
            },
            "require": {
                "default": "./dists/index.cjs",
                "types": "./dists/index.d.cts"
            }
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/morrighan/packages.git",
        "directory": "packages/sass-bridge"
    },
    "dependencies": {
        "chroma-js": "^3.1.2",
        "core-js-pure": "^3.39.0",
        "reflect-metadata": "^0.2.2",
        "sass": "npm:sass-embedded@^1.82.0"
    },
    "devDependencies": {
        "@cichol/sass-bridge": "file:."
    },
    "engines": {
        "node": "^20.18.0",
        "npm": "^10.9.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
