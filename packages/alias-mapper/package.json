{
    "name": "@cichol/alias-mapper",
    "version": "4.4.4",
    "description": "A resolver in order to shorten deeply nested relative path expression.",
    "keywords": [
        "babel",
        "babel-plugin",
        "module",
        "resolver",
        "alias",
        "rewrite",
        "resolve",
        "rename",
        "mapping",
        "import",
        "eslint",
        "eslint-plugin-import",
        "eslint-import-resolver"
    ],
    "homepage": "https://github.com/morrighan/packages",
    "bugs": {
        "url": "https://github.com/morrighan/packages/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "type": "module",
    "main": "./dists/index.cjs",
    "module": "./dists/index.js",
    "types": "./dists/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "default": "./dists/index.js",
                "types": "./dists/index.d.ts"
            },
            "require": {
                "default": "./dists/index.cjs",
                "types": "./dists/index.d.cts"
            }
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/morrighan/packages.git",
        "directory": "packages/alias-mapper"
    },
    "scripts": {
        "build": "npm run build:tsc",
        "build:tsc": "npx tsc --project sources --outDir dists"
    },
    "dependencies": {
        "@babel/helper-plugin-utils": "^7.14.5",
        "core-js-pure": "^3.39.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "lodash": "^4.17.21"
    },
    "peerDependencies": {
        "@babel/core": "^7.16.7",
        "eslint": "^8.57.0 || ^9.10.0"
    },
    "engines": {
        "node": "^20.18.0",
        "npm": "^10.9.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
