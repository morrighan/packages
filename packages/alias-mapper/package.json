{
    "name": "@cichol/alias-mapper",
    "version": "5.0.0",
    "description": "A resolver in order to shorten deeply nested relative path expression.",
    "keywords": [
        "babel",
        "babel-plugin",
        "module",
        "resolver",
        "alias",
        "rewrite",
        "resolve",
        "rename",
        "mapping",
        "import",
        "eslint",
        "eslint-plugin-import",
        "eslint-import-resolver"
    ],
    "homepage": "https://github.com/morrighan/packages",
    "bugs": {
        "url": "https://github.com/morrighan/packages/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "type": "module",
    "main": "./dists/index.cjs",
    "module": "./dists/index.js",
    "types": "./dists/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "default": "./dists/index.js",
                "types": "./dists/index.d.ts"
            },
            "require": {
                "default": "./dists/index.cjs",
                "types": "./dists/index.d.cts"
            }
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/morrighan/packages.git",
        "directory": "packages/alias-mapper"
    },
    "dependencies": {
        "@babel/helper-plugin-utils": "^7.27.1",
        "@babel/runtime-corejs3": "^7.27.1",
        "core-js-pure": "^3.42.0",
        "eslint-import-resolver-typescript": "^4.3.4"
    },
    "peerDependencies": {
        "@babel/core": "^7.27.1",
        "eslint": "^9.26.0"
    },
    "peerDependenciesMeta": {
        "@babel/core": {
            "optional": true
        },
        "eslint": {
            "optional": true
        }
    },
    "engines": {
        "node": "^20.19.1 || ^22.15.0",
        "npm": "^11.3.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
