{
    "name": "@cichol/eslint-config",
    "version": "5.2.0",
    "description": "An universal ESLint configuration for personal projects.",
    "homepage": "https://github.com/morrighan/packages",
    "bugs": {
        "url": "https://github.com/morrighan/packages/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "type": "module",
    "main": "index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/morrighan/packages.git",
        "directory": "packages/eslint-config"
    },
    "dependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
        "@eslint/compat": "^1.4.0",
        "@eslint/eslintrc": "^3.3.1",
        "@stylistic/eslint-plugin": "^5.4.0",
        "deepmerge": "^4.3.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^6.1.0",
        "typescript-eslint": "^8.45.0"
    },
    "peerDependencies": {
        "eslint": "^9.36.0",
        "typescript": "^5.9.3"
    },
    "peerDependenciesMeta": {
        "eslint": {
            "optional": true
        },
        "typescript": {
            "optional": true
        }
    },
    "overrides": {
        "eslint-config-airbnb": {
            "eslint": "$eslint",
            "eslint-config-airbnb-base": {
                "eslint": "$eslint"
            },
            "eslint-plugin-react-hooks": "$eslint-plugin-react-hooks"
        },
        "eslint-config-airbnb-base": {
            "eslint": "$eslint"
        }
    },
    "engines": {
        "node": "^20.19.5 || ^22.20.0",
        "npm": "^11.6.1"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
